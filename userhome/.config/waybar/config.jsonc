    "format": "ÔÉâ {used:0.1f}G",
    "format-alt": "ÔÉâ {}%",
    "max-length": 10
  },
  "backlight": {
    "device": "eDP-1",
    "format": "{icon} {percent}",
    "format-icons": [
      "Û±©é",
      "Û±©è",
      "Û±©ê",
      "Û±©ë",
      "Û±©í",
